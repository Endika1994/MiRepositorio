<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>com.ipartek.formacion.controladores.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ProyectoServlet</servlet-name>
  	<display-name>ProyectoServlet</display-name>
  	<description></description>
  	<servlet-class>com.ipartek.formacion.controladores.ProyectoServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>NuevoProyectoServlet</servlet-name>
  	<display-name>NuevoProyectoServlet</display-name>
  	<description></description>
  	<servlet-class>com.ipartek.formacion.controladores.NuevoProyectoServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>GuardarProyectoServlet</servlet-name>
  	<display-name>GuardarProyectoServlet</display-name>
  	<description></description>
  	<servlet-class>com.ipartek.formacion.controladores.GuardarProyectoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ProyectoServlet</servlet-name>
  	<url-pattern>/ProyectoServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>NuevoProyectoServlet</servlet-name>
  	<url-pattern>/NuevoProyectoServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>GuardarProyectoServlet</servlet-name>
  	<url-pattern>/GuardarProyectoServlet</url-pattern>
  </servlet-mapping>
</web-app>
